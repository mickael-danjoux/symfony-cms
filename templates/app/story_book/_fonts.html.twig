<div class="accordion-item">
    <h2 class="accordion-header">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFonts" aria-expanded="false" aria-controls="collapseFonts">
            Fonts
        </button>
    </h2>
    <div id="collapseFonts" class="accordion-collapse collapse " aria-labelledby="headingFonts" data-bs-parent="#accordionStoryBook">
        <div class="accordion-body">
            {%for key,value in fontsClasses%}
                <div class="row mb-2">
                    <div class="col-12 col-md-6">
                        <{{key}}>{{value}}<{{key}}>
                    </div>
                    <div class="col-12 col-md-6 d-flex">
                        <code id="font-{{loop.index}}">&lt;{{key}}&gt;{{value | capitalize}}&lt;/{{key}}&gt;
                            <button class="bg-transparent border-0 copy" @click="copyContent('font-{{loop.index}}')">
                            </button>
                        </code>
                    </div>
                </div>
                <hr>
            {%endfor%}
        </div>
    </div>
</div>
